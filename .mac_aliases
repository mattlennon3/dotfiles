# Default browser
alias dbf="osascript $HOME/scripts/mac-os-utils/switch-browser/switch-browser.scpt 'firefox'"
alias dbff=dbf
alias dbfd="osascript $HOME/scripts/mac-os-utils/switch-browser/switch-browser.scpt 'firefoxdeveloperedition'"
alias dbw=dbfd

# React Native / Simulator
alias androidLink="$HOME/scripts/android-adb/androidLink.sh"
alias droid="npm run android"
alias adebug="adb shell input keyevent 82"



