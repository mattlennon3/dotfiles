
export CONVERGE_DIRECTORY="$HOME/git/converge"
export CONVERGE_APP="$HOME/git/converge/mobile-app"

# cd Aliases
alias cdc="cd $CONVERGE_DIRECTORY"
alias cdma="cd $CONVERGE_APP"

# Quick builds
alias uma="(cd $CONVERGE_APP; npm install; (cd ios; pod install))"
alias ddd="rm -rf $HOME/Library/Developer/Xcode/DerivedData"
alias sde=". $HOME/git/converge/deploy/set-dev-env.sh"
alias mks=". $HOME/git/converge/keystore.sh"

# Call sde and mks so I don't have to on every new terminal..
sde
mks

