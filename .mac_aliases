
# Mac System Dirs
export cdicl="cd \"$HOME/Library/Mobile Documents/com~apple~CloudDocs/\""

# Converge
export FASTNED_DIRECTORY="$HOME/git/fastned"
export FASTNED_APP="$FASTNED_DIRECTORY/fastnedapp"

# cd Aliases
alias cdf="cd $FASTNED_DIRECTORY"
alias cdma="cd $FASTNED_APP"

# Quick builds
alias uma="(cd $FASTNED_APP; npm install; (cd ios; pod install))"
alias ddd="rm -rf $HOME/Library/Developer/Xcode/DerivedData"

# Play a sound (for command completions)"
alias alert="afplay /System/Library/Sounds/Ping.aiff"

# Default browser
alias dbf="osascript $HOME/scripts/mac-os-utils/switch-browser/switch-browser.scpt 'firefox'"
alias dbff=dbf
alias dbfd="osascript $HOME/scripts/mac-os-utils/switch-browser/switch-browser.scpt 'firefoxdeveloperedition'"
alias dbw=dbfd

# React Native / Simulator
alias androidLink="$HOME/scripts/android-adb/androidLink.sh"
alias droid="npm run android"
alias adebug="adb shell input keyevent 82"
alias check="npm run test; npm run typecheck; npm run lint; alert; $*"




